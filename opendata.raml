                    "FullLegalName": {
                      "description": "Name by which a party is known and which is usually used to identify that party.",
                      "type": "string",
                      "minLength": 1,
                      "maxLength": 350
                    }
                  },
                  "required": [
                    "FullLegalName"
                  ],
                  "additionalProperties": false
