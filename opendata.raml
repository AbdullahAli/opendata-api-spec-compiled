                                        "EveryFiveBusinessDays",
                                        "Item",
                                        "Monthly",
                                        "OnAccountAnniversary",
                                        "PerHour",
                                        "PerOccurrence",
                                        "PerSheet",
                                        "PerTransactionAmount",
                                        "PerTransactionPercentage",
                                        "Quarterly",
                                        "SixMonthly",
                                        "StartOfLoan",
                                        "StatementMonthly",
                                        "Weekly",
                                        "Yearly"
