                          "pattern": "^[0-9]{4}-(0[1-9]|(1[1|2]))-((0[1-9])|((1|2)[0-9])|(30|31))$"
                        },
                        "LengthPromotionalInDays": {
                          "description": "Describes the length if only a duration is given instead of a date",
